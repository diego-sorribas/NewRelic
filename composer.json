{
    "name": "neeckeloo/newrelic",
    "description": "NewRelic module for Zend Framework",
    "type": "library",
    "keywords": [
        "zf2", "newrelic"
    ],
    "license": "MIT",
    "homepage": "https://github.com/neeckeloo/NewRelic",
    "authors": [
        {
            "name": "Nicolas Eeckeloo",
            "email": "neeckeloo@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/container": "1.0",
        "monolog/monolog": "^1.17 || ^2.0",
        "zendframework/zend-console": "^2.6",
        "zendframework/zend-eventmanager": "^2.6 || ^3.0",
        "zendframework/zend-servicemanager": "^2.7 || ^3.0",
        "zendframework/zend-http": "^2.5",
        "zendframework/zend-mvc": "^2.7 || ^3.0",
        "zendframework/zend-stdlib": "^3.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.17 || ^8.4.3",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "conflict": {
        "container-interop/container-interop": "<1.2"
    },
    "autoload": {
        "psr-4": {
            "NewRelic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NewRelicTest\\": "tests/"
        }
    }
}
